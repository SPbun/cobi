#include <iostream>

void foo() {
	std::cout << "executing foo()\n";
}

void bar() {
	std::cout << "executing bar()\n";
}

int main(int argc, char** argv) {
	std::cout << "=== start main  ===\n";
	
	for(int i = 0; i < 15; ++i) {
		foo();
	}

	for(int i = 0; i < 10; ++i) {
                bar();
        }

	std::cout << "=== finish main ===\n";
	return 0;
}
