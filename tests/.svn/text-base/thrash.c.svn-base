#include <stdio.h>

double get55() {
	return 55.;
}

float get22() {
	return 22.;
}

int main(int argc, char** argv) {

	printf("calling get22() to init float f value.\n");

	float f = get22();


	if(f==22.) {
		printf("f equals 22.\n");
	}
	else {
		printf("f is %f but should be 22.\n",f);
	}

	printf("\ncalling get55() to init double d value. \n");

	double d = get55();
	
	if(d==55.) {
		printf("d is 55.\n");
	}
	else {
		printf("d is %f but should be 55.\n",d);
	}

}
